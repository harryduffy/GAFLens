datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model ManagerMeeting {
  id                 Int      @id @default(autoincrement())
  managerName        String
  lastMeetingDate    DateTime
  region             String
  currency           String
  AUM                BigInt
  gafAttendees       String
  externalAttendees  String
}

