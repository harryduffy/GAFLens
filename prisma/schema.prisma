datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Manager {
  id                 Int      @id @default(autoincrement())
  managerName        String
  region             String
  currency           String
  AUM                BigInt
}

model ManagerMeetingDetail {
  id                 Int      @id @default(autoincrement())
  managerName        String
  managerCountry     String
  meetingDate        DateTime
  managerAUM         BigInt
  fundName           String
  fundSize           BigInt
  assetClasses       String
  investmentStrategies String
  fundGeographicFocus String
  fundTargetNetReturn Int
  gafAttendees       String
  externalAttendees  String
  notes              String
}


